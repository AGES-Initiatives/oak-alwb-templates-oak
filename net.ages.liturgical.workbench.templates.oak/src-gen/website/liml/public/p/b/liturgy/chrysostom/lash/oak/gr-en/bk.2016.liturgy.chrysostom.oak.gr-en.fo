	</fo:block></fo:table-cell>
	<fo:table-cell 
	padding-left='5px' 
	padding-right='0px' 
	vertical-align='top'><fo:block keep-with-next.within-page='always' 
	color='red' 
	font-family='Palatino Linotype Bold' 
	font-size='11pt' 
	font-variant='normal' 
	font-weight='bold' 
	padding-after='3px' 
